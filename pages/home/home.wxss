/* pages/home/home.wxss */
Page {
  overflow: hidden;
}

.content {
  height: 100%;
}

.top-content {
  display: flex;
  margin-bottom: 10px;
}

.top-image {
   width: 100%; 
  padding-left: 5px;
  padding-right: 5px;
}

.top-content text {
  position: absolute;
  top: 30px;
  width: 100%;
  text-align: center;
  color: #fff;
  font-weight: bold;
}

.cellList {
  border-radius: 5px;
}

.itemCell {
  height: 80px;
}